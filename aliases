
#!/bin/bash

alias lk='clear && ls -lart'
alias cdd='cd ..'
alias md='mkdir -p'
alias h='history'
#alias kga='kubectl get all'
alias kda='kubectl delete all --all'

function kga
{
 array_list=("nodes" "pods" "rs" "svc" "deployments");
 for object in ${array_list[@]}
 do 
    echo "<----------------kubectl get ${object}---------------->"
    kubectl get ${object}
done
}


echo Aliases set......
